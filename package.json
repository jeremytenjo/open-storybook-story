{
	"name": "open-storybook-story",
	"displayName": "Quick File Create",
	"description": "Automatically create file when folder is created",
	"publisher": "tenjojeremy",
	"version": "0.16.0",
	"engines": {
		"vscode": "^1.55.0"
	},
	"categories": [
		"Other"
	],
	"activationEvents": [
		"onStartupFinished"
	],
	"main": "./src/index.js",
	"scripts": {
		"lint": "eslint .",
		"pretest": "npm run lint",
		"test": "",
		"publish": "vsce publish minor",
		"vscode:prepublish": "npm run test"
	},
	"icon": "assets/images/logo.png",
	"repository": {
		"type": "git",
		"url": "https://github.com/jeremytenjo/open-storybook-story"
	},
	"contributes": {
		"configuration": {
			"title": "openStorybookStory",
			"properties": {
				"openStorybookStory.disable": {
					"type": "boolean",
					"default": false,
					"description": "Disable extension"
				},
				"openStorybookStory.dirToWatch": {
					"type": "string",
					"default": "src/**",
					"description": "Directory to watch for new folders (blob)"
				},
				"openStorybookStory.ignoreFolders": {
					"type": "array",
					"default": [
						"node_modules"
					],
					"description": "Folders to ignore adding files to (separated by commas) eg build, dist"
				},
				"openStorybookStory.fileNameCase": {
					"type": "string",
					"default": "camelCase",
					"enum": [
						"camelCase",
						"capitalCase",
						"constantCase",
						"dotCase",
						"headerCase",
						"noCase",
						"paramCase",
						"pascalCase",
						"pathCase",
						"sentenceCase",
						"snakeCase"
					],
					"description": "Created file name case"
				},
				"openStorybookStory.fileExtension": {
					"type": "string",
					"default": ".js",
					"description": "Created file extension"
				},
				"openStorybookStory.outputIndexjs": {
					"type": "boolean",
					"default": false,
					"description": "Create index.js"
				}
			}
		}
	},
	"devDependencies": {
		"@types/vscode": "^1.55.0",
		"@types/glob": "^7.1.3",
		"@types/mocha": "^8.0.4",
		"@types/node": "^12.11.7",
		"eslint": "^7.19.0",
		"glob": "^7.1.6",
		"mocha": "^8.2.1",
		"typescript": "^4.1.3",
		"vscode-test": "^1.5.0"
	},
	"dependencies": {
		"change-case": "^4.1.2",
		"os": "^0.1.1"
	}
}